{
  "short_name": "Eduauto",
  "name": "Eduauto_from_Hovilo",
  "action": {},
  "version": "3.0.0",
  "description": "This automatic grading tool on edunext",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://fu.edunext.vn/*"],
      "js": ["main.js"]
    }
  ]
,
  "web_accessible_resources":[{
    "resources": ["*.js","doc_script/*.js"],
    "matches": ["https://fu.edunext.vn/*"],
    "use_dynamic_url": true
  }]
}
